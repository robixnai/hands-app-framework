# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'

target 'HandsApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HandsApp
  pod 'UIScrollView-InfiniteScroll', '~> 1.0.0'
  pod 'SVProgressHUD'

  target 'HandsAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HandsAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'HandsFramework' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HandsFramework
  pod 'AFNetworking', '~> 3.0'

  target 'HandsFrameworkTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
